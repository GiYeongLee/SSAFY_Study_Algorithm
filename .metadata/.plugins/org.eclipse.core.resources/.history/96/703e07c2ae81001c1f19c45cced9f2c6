package com.ssafy.im;

import java.util.Scanner;

public class IM_2047_신문헤드라인 {
	public static void main(String[] args) {	
		Scanner in = new Scanner(System.in);	
		String str = in.nextLine();
		
		for(int i = 0; i < str.length(); i++) {	
			char a = str.charAt(i);
			if(a >= 'a' && a <= 'z') {
				a += 'a';
			}
			System.out.print(a);
		}
	}
}
