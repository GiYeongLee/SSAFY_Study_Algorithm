package com.ssafy.im;

import java.util.Scanner;

public class IM_2005_파스칼의삼각형 {
	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int[] arr = new int[10];
		int count = 1;
		arr[0] = 1;
		System.out.print(arr[j] + " ");
		
		int N = sc.nextInt();
		
		for(int i = 0; i < N; i++) {
			for(int j = 1; j < N; j++) {
				arr[j] = arr[j - 1] + arr[j];
				System.out.print(arr[j] + " ");
			}
			System.out.println();
		}
		
	}
}
